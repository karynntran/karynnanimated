{
  "name": "redux-simple-starter",
  "version": "1.0.0",
  "description": "Simple starter package for Redux with React and Babel support",
  "main": "index.js",
  "repository": "git@github.com:StephenGrider/ReduxSimpleStarter.git",
  "scripts": {
    "dev": "concurrently node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js \"npm run watch-css\"",
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js --recursive ./test",
    "test:watch": "npm run test -- --watch",
    "postinstall": "webpack -p",
    "build-css": "node-sass --include-path scss style/style.scss style/style.css",
    "watch-css": "nodemon -e scss -x \"npm run build-css\"",
    "start": "node server.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.2.1",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babel-preset-stage-1": "^6.22.0",
    "chai": "^3.5.0",
    "chai-jquery": "^2.0.0",
    "concurrently": "^3.5.1",
    "create-react-class": "^15.6.3",
    "css-loader": "^0.28.0",
    "jquery": "^2.2.1",
    "jsdom": "^8.1.0",
    "mocha": "^2.4.5",
    "node-sass": "^4.5.2",
    "nodemon": "^1.17.3",
    "prop-types": "^15.6.1",
    "react": "^0.14.8",
    "react-addons-test-utils": "^0.14.8",
    "react-dom": "^0.14.8",
    "react-slider": "^0.11.2",
    "style-loader": "^0.16.1",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.14.0"
  },
  "dependencies": {
    "babel-core": "^6.2.1",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "chai": "^3.5.0",
    "chai-jquery": "^2.0.0",
    "css-loader": "^0.28.0",
    "jquery": "^2.2.1",
    "jsdom": "^8.1.0",
    "mocha": "^2.4.5",
    "node-sass": "^4.5.2",
    "react": "^0.14.8",
    "react-addons-test-utils": "^0.14.8",
    "react-dom": "^0.14.8",
    "react-easy-swipe": "0.0.8",
    "style-loader": "^0.16.1",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.14.0"
  }
}
